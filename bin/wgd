#!perl
package
    App::WGDev;
# ABSTRACT: WebGUI Developer Utilities
use strict;
use warnings;
use 5.008008;

use WGDev::Command;

our $PACKED;
our @PACKED;

my $return = do { WGDev::Command->run(@ARGV) };
if ( my $message = $@ ) {
    $message =~ s/\n?\z/\n/msx;
    die $message;
}
exit( $return ? 0 : 1 );
